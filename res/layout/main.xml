<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <ProgressBar style="@android:style/Widget.ProgressBar.Horizontal" android:id="@+id/progress" android:layout_height="wrap_content" android:layout_width="match_parent"></ProgressBar>
    <TableLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:id="@+id/tableLayout1">
        <TableRow android:id="@+id/row1" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <Button android:layout_width="wrap_content" android:text="Start" android:id="@+id/discover" android:layout_height="wrap_content"></Button>
            <Button android:layout_width="wrap_content" android:text="Stop" android:id="@+id/stop" android:layout_height="wrap_content"></Button>
            <EditText android:layout_width="wrap_content" android:id="@+id/from" android:layout_height="wrap_content" android:text="EditText"></EditText>
            <EditText android:layout_width="wrap_content" android:id="@+id/to" android:layout_height="wrap_content" android:text="EditText"></EditText>
        </TableRow>
        
    </TableLayout>
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_height="wrap_content" android:layout_width="match_parent">
        <ListView android:id="@+id/listView1" android:layout_height="250dip" android:layout_width="160dip"></ListView>
        <EditText android:layout_width="wrap_content" android:id="@+id/to" android:layout_height="wrap_content" android:text="EditText" android:layout_above="@+id/listView1" android:layout_alignLeft="@+id/listView1"></EditText>
        <TextView android:id="@+id/results" android:layout_width="160dip" android:text="logs" android:layout_height="200dip" android:layout_toRightOf="@+id/listView1" android:layout_alignTop="@+id/listView1" android:layout_alignBottom="@+id/listView1"></TextView>
    </RelativeLayout>
</LinearLayout>
